{
    "name": "Yt click and subscribe",
    "author": "hello@abdulmoiz.net",
    "description": "The extension that click and subscribe to the youtube channel of first video.", 
    "version": "0.0.1",
    "manifest_version": 3,
    "icons": { 
        "16": "images/icon.png",
        "48": "images/icon.png",
        "128": "images/icon.png"
    },
    "action": {
        "default_title": "First Extension",
        "default_popup": "popup/popup.html",
        "default_icon": {
            "16": "images/icon.png",
            "48": "images/icon.png",
            "128": "images/icon.png"
        }
    }, 
    "permissions": [
        "activeTab"
          ],
  
    "background":{
        "service_worker": "background/background.js"
    }

    }

